dos big_data = 1000.75;

// Main function
sel main() {
    // Create instance
    var john = Person("John Doe", 30);
    john.greet();
    
    // Display variables
    print("Simple var: ", simple_var);
    print("Network var: ", network_var);
    print("Logger var: ", logger_var);
    print("Big data: ", big_data);
    
    // Show sizes
    print("Size of simple_var: ", size(simple_var), " bytes");
    print("Size of network_var: ", size(network_var), " bytes");
    print("Size of logger_var: ", size(logger_var), " bytes");
    print("Size of big_data: ", size(big_data), " bytes");
    
