// SwiftFlow Example - Fusion CLAIR & SYM

// Types CLAIR
class Person {
    name: string;
    age: int;
    
    init(name: string, age: int) {
        this.name = name;
        this.age = age;
    }
    
    func greet() {
        print("Hello, my name is " + this.name);
    }
}

// Types SYM
import() {
