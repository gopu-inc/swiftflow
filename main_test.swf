// Fonction avec plusieurs instructions
func complex_function(x) {
    var result = x * 2;
    
    if (result > 10) {
        result = result - 5;
    } else {
        result = result + 5;
    }
    
    var i = 0;
    while (i < 3) {
        result = result + 1;
        i = i + 1;
    }
    
    return result;
}

var complex_result = complex_function(8);
